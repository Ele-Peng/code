

.tagGrid {
width: 690rpx;
margin-left: 30rpx;
margin-bottom: 20rpx;
margin-top: 30rpx;
display: flex;
flex-direction: column;
}

.tagTitle {
  color:#FF9906;
  font-size: 28rpx;
  text-align: center;
  margin-top: 25rpx;
  background-color: white;
}

.tagChoseArea {
  display: flex;
  flex-direction: row;
  flex-wrap:wrap;
  justify-content:center;
  background-color: white;
}

.tagArea {
  width: 230rpx;
}

.tagSymbol {
  width: 46rpx;
  height: 10rpx;
  margin-top: 36rpx;
  margin-left: 93rpx;
  border-radius: 10rpx;
  display: flex;
  justify-content: center;
}

.tagSymbolTitle {
  text-align: center;
}

.tagSymbolTitle text {
  font-size: 24rpx;
  color: #3B424C;
}